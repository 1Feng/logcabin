Import('env', 'object_files')

src = [
]
object_files['Protocol'] = env.StaticObject(src)

env.Protoc([],
           "Client.proto",
           PROTOCPROTOPATH = ['.'],
           PROTOCPYTHONOUTDIR = None,
           PROTOCOUTDIR = '.')

