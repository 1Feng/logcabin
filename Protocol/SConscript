Import('env')

src = [
    "#build/Protocol/Client.pb.o",
]

env.StaticLibrary("LogCabinProtocol",
                  src,
                  CPPPATH = [ "#" ])

env.Protoc([],
           "Client.proto",
           PROTOCPROTOPATH = ['.'],
           PROTOCPYTHONOUTDIR = None,
           PROTOCOUTDIR = '.')

env.StaticObject("#build/Protocol/Client.pb.o",
                 "#build/Protocol/Client.pb.cc",
                 CPPPATH = "#",
                 CXXFLAGS = "-std=c++0x")
